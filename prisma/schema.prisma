generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Team {
  id       String   @id @default(uuid())
  name     String
  teamLogo String?
  players  Player[]
}

model Player {
  id          String  @id @default(uuid())
  name        String
  pot         Int
  playerImage String?
  teamId      String?
  team        Team?    @relation(fields: [teamId], references: [id])
}
